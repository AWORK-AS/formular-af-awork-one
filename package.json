{
    "name": "formular-af-awork-one",
    "version": "1.0.4",
    "description": "Formular af AWORK ONE",
    "author": "Mindell Zamora <mz@awork.dk>",
    "files": [
        "ajax",
        "assets",
        "backend",
        "cli",
        "engine",
        "frontend",
        "functions",
        "integrations",
        "internals",
        "languages",
        "rest",
        "templates",
        "vendor",
        "index.php",
        "LICENSE.txt",
        "formular-af-awork-one.php",
        "README.txt",
        "uninstall.php"
    ],
    "scripts": {
        "start": "wp-scripts start",
        "build": "wp-scripts build",
        "check-engines": "wp-scripts check-engines",
        "check-licenses": "wp-scripts check-licenses",
        "format": "wp-scripts format",
        "lint:css": "wp-scripts lint-style ./assets/src/styles/**/*.scss --fix",
        "lint:js": "wp-scripts lint-js ./assets/src",
        "lint:md:docs": "wp-scripts lint-md-docs",
        "lint:pkg-json": "wp-scripts lint-pkg-json",
        "packages-update": "wp-scripts packages-update",
        "plugin-zip": "wp-scripts plugin-zip",
        "postinstall": "npm run build",
        "publish": "npm run build && npm run plugin-zip",
        "wp-env": "wp-env start"
    },
    "devDependencies": {
        "@types/babel__core": "^7.20.5",
        "@types/jquery": "^3.5.16",
        "@types/jqueryui": "^1.12.16",
        "@types/wordpress__block-editor": "^11.5.17",
        "@types/wordpress__blocks": "^12.5.18",
        "@wordpress/block-editor": "^15.2.0",
        "@wordpress/scripts": "^30.22.0",
        "typescript": "^5.9.2"
    },
    "engines": {
        "node": ">=24.6.0",
        "npm": ">=11.5.1"
    },
    "browserslist": [
        "extends @wordpress/browserslist-config"
    ],
    "eslintConfig": {
        "extends": [
            "plugin:@wordpress/eslint-plugin/recommended",
            "plugin:jsdoc/recommended"
        ],
        "env": {
            "browser": true,
            "es6": true,
            "jquery": true
        },
        "parser": "@typescript-eslint/parser",
        "globals": {
            "jQuery": true,
            "$": true
        }
    },
    "stylelint": {
        "extends": [
            "@wordpress/stylelint-config",
            "@wordpress/stylelint-config/scss"
        ]
    },
    "prettier": "@wordpress/prettier-config",
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    }
}
