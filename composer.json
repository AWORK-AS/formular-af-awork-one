{
    "name": "mzawork/formular_af_aworkone",
    "description": "Wordpress Plugin for AWORK One journalsystem",
    "license": "GPL-3.0-or-later",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Mindell Zamora",
            "email": "mz@awork.dk",
            "homepage": "https://github.com/mz-aworkdk"
        }
    ],
    "homepage": "https://awork.dk",
    "require": {
        "php": ">=7.4",
        "composer/installers": "^2.3.0",
        "micropackage/requirements": "^1.2",
        "wpbp/debug": "~1.1.0",
        "wpbp/i18n-notice": "dev-main",
        "wpdesk/wp-notice": "^3.2",
        "inpsyde/wp-context": "^1.5",
        "sniccowp/php-scoper-wordpress-excludes": "^6.8"
    },
    "require-dev": {
        "codeatcode/codeatcs": "^1.0",
        "dealerdirect/phpcodesniffer-composer-installer": "1.0.0",
        "ergebnis/composer-normalize": "^2.6",
        "lucatume/function-mocker": "^1.0",
        "phpro/grumphp": "^1.13",
        "szepeviktor/phpstan-wordpress": "^1.1.6",
        "wp-coding-standards/wpcs": "dev-develop",
        "php-stubs/wp-cli-stubs": "^2.12"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mte90/wp-super-duper",
                "version": "dev-fork",
                "autoload": {
                    "files": [
                        "wp-super-duper.php"
                    ]
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/mte90/wp-super-duper.git",
                    "reference": "fork"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "ajax/",
            "backend/",
            "cli/",
            "engine/",
            "frontend/",
            "integrations/",
            "internals/",
            "internals/models/",
            "internals/views/",
            "rest/"
		]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "phpro/grumphp": true
        },
		"autoloader-suffix": "formular_af_aworkone"
    }
}
